<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scut.demo.mapper.TestInfoMapper">

    <insert id="insertChoiceQuestion" parameterType="com.scut.demo.entity.TestInfo">
        insert into choicequestion(choice_queid, title,ans,op_A,op_B,op_C,op_D) values (#{id}, #{name},#{correct}, #{op_A},#{op_B},#{op_C},#{op_D})
    </insert>

    <select id="getQuestionTextById" resultType="com.scut.demo.entity.TestInfo">
        select * from test where test_id=#{id}
    </select>

</mapper>